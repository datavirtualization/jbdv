   <!-- Leave this as the first shortcut entry.-->
   <shortcut
     name="Uninstall Platform"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     iconIndex="0"
     target="$JAVA_HOME\bin\java"
     commandLine="-jar &#34;$INSTALL_PATH\Uninstaller\uninstaller.jar&#34;"
     iconFile="$INSTALL_PATH\${common.eap.name}\icons\48-uninstall.ico"
     description="Uninstall the JBoss Platform"
     type="Application"
     encoding="UTF-8"
     terminal="false"
     KdeSubstUID="false"
     initialState="normal"
     categories=""
     createForAll="true" />

